[run]
source = .
omit = 
    */env/*
    */venv/*
    */.venv/*
    */tests/*
    */test_*
    */__pycache__/*
    */site-packages/*
    setup.py
    */dist/*
    */build/*
    */.pytest_cache/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING: